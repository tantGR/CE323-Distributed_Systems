#SIMPLESCRIPT
	BEQ $argc 2 #LSTART
	PRN "Wrong number of arguments"
	RET 
#LSTART	PRN "Hello from execution nr " $arg0
	PRN "Summing up from 1 to " $arg1
	SET $n 1
	SET $sum 0
#LOOP	BGT $n $arg1 #LEND
	ADD $sum $sum $n
	ADD $n $n 1
	SLP 140
	BRA #LOOP
#LEND	SLP 100	
	PRN "WAITING"
	RCV 1 2
	PRN "SEND 3"
	SND 1 3
	PRN "Result is " $sum
	RET
