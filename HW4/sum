#SIMPLESCRIPT
	BEQ $argc 2 #LSTART
	PRN "Wrong number of arguments"
	RET 
#LSTART	PRN "Hello from execution nr " $arg0
	PRN "Summing up from 1 to " $arg1
	SET $n 1
	SET $sum 0
#LOOP	BGT $n $arg1 #LEND
	ADD $sum $sum $n
	ADD $n $n 1
	BRA #LOOP
#LEND	SLP 1000
	RCV 1 1
	PRN "RECEIVED"
	PRN "Result is " $sum
	RET
